{
  "name": "iki-bowl",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "check:audio": "node scripts/check-audio-duration.mjs",
    "check:audio:set": "node scripts/check-audio-set.mjs",
    "check:audio:size": "node scripts/check-audio-size.mjs",
    "check:branding": "node scripts/check-branding.mjs",
    "check:assets": "npm run check:audio:set && npm run check:audio:size"
